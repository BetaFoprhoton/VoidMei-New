common{
  condensation_type:t="rain"
  dirt_type:t="ground"

  water{
    cloud_shadow_intensity:r=0.8
    L_inv_white:r=0.3
  }

  hdr{
    useAdaptation:b=yes
    adaptationTimeHighToNorm:r=0.25
    adaptationTimeNormToHigh:r=0.65
    adaptationMultiplier:r=0.65
    targetBrightnessUp:r=1.005
    targetBrightnessEffect:r=0.0

    fake{
      adaptationMax:r=2.2
      adaptationMin:r=0.65
      lumMin:r=0.68
      lumMax:r=0.95
    }
  }

  DemonPostFx{
    use:b=yes
    showAlpha:b=no
    glowRadius:r=0.1
    useBetterGlow:b=yes
    glowFadeK:r=2.0
    glowEdgeK:r=0.0
    glowAngleU:r=44.0
    glowAngleV:r=144.0
    useRawSkyMask:b=yes
    hdrStarThreshold:r=4.75
    hdrDarkThreshold:r=0.6
    hdrGlowPower:r=1.0
    hdrGlowMul:r=0.2
    hdrStarPowerMul:r=0.32

    starGlow{
      numRays:i=3

      defaultRayParams{
        radius:r=0.03
        power:r=1.0
        fadeK:p3=5.0, 1.5, 0.0
        color:c=255, 210, 150, 255
      }

      ray01{
        angle:r=99.0
        useCustom:b=no
        radius:r=0.03
        power:r=2.0
        fadeK:p3=2.0, 2.0, 1.0
        color:c=255, 255, 255, 255
      }

      ray02{
        angle:r=39.0
        useCustom:b=no
        radius:r=0.051
        power:r=4.0
        fadeK:p3=2.0, 2.0, 1.0
        color:c=255, 255, 255, 255
      }

      ray03{
        angle:r=-21.0
        useCustom:b=no
        radius:r=0.047
        power:r=4.0
        fadeK:p3=2.0, 2.0, 1.0
        color:c=255, 255, 255, 255
      }

      ray04{
        angle:r=0.0
        useCustom:b=yes
        radius:r=0.061
        power:r=1.0
        fadeK:p3=2.0, 2.0, 1.0
        color:c=255, 255, 255, 255
      }
    }
  }
}