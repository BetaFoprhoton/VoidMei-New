Weapon{
  trigger:t="gunner0"
  triggerGroup:t="primary"
  blk:t="gameData/Weapons/groundModels_weapons/25mm_2M_3_user_cannon.blk"
  emitter:t="mg_barrel_01"
  flash:t="emtr_gun_flame_01"
  useEmitter:b=yes
  recoilOffset:r=0.2
  barrelDP:t="mg_01_dm"
  breechDP:t="mg_01_dm"
  defaultYaw:r=0.0
  defaultPitch:r=0.0
  speedYaw:r=60.0
  speedPitch:r=45.0
  fireConeAngle:r=1.0
  bullets:i=780
  speedYaw:r=45.0
  speedPitch:r=45.0
  fireConeAngle:r=20.0
  ChainfireTime:r=2.0
  DelayAfterShoot:r=2.0
  aimMinDist:r=20.0
  aimMaxDist:r=4000.0
  randomAngle:r=0.0
  accuracyAir:r=0.5
  accuracyGnd:r=0.5
  errMeasureVel:r=0.0
  errTargettingOn100kmph:r=0.0
  errMeasureVelFast:r=100.0
  errMeasureVelFwdShift:r=1.0
  errMeasureVelDir:r=3.0
  errTargetting:r=1.0
  errExplTime:r=0.0
  reuseYawTrigger:i=10
  reusePitchTrigger:i=10

  turret{
    head:t="turret_h_01"
    gun:t="mg_01"
    barrel:t="mg_barrel_01"
    gunnerDm:t="gunner_01_dm"
    barrel:t="mg_01_dm"
    verDriveDm:t="drive_turret_v_01_dm"
    horDriveDm:t="drive_turret_h_01_dm"
  }

  limits{
    yaw:p2=-160.0, 130.0
    pitch:p2=-10.0, 85.0
  }

  limitsTable{
    lim1:p4=-160.0, -85.0, 2.0, 85.0
    lim2:p4=-85.0, 85.0, -10.0, 85.0
    lim3:p4=85.0, 130.0, 2.0, 85.0
  }
}

