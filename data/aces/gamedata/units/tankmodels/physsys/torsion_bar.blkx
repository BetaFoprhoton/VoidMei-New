PhysSys{
  find:t="^bone_suspension_(._\d\d)$"

  points{

    suspension{
      name:t="bone_suspension_$1"
    }

    wheel{
      name:t="bone_wheel_$1"
      limitMin:p3=-1000.0, 0.0, 0.0
      limitMax:p3=1000.0, 0.0, 0.0
    }
  }

  constraints{

    edge{
      from:t="suspension"
      to:t="wheel"
    }
  }
}