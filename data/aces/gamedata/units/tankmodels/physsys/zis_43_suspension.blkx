PhysSys{
  find:t="^bone_suspension_(\w)_(\d+)$"

  points{

    lever{
      name:t="bone_suspension_$1_$2"
      limitMin:p3=0.0, 0.0, 0.0
      limitMax:p3=0.0, 0.0, 0.0
    }

    suspension_01_01{
      name:t="bone_suspension_$1_$2_01"
      limitMin:p3=-0.5, -0.5, 0.0
      limitMax:p3=0.5, 0.5, 0.0
    }

    suspension_01_02{
      name:t="bone_suspension_$1_$2_02"
      limitMin:p3=-0.5, -0.5, 0.0
      limitMax:p3=0.5, 0.5, 0.0
    }

    wheel_01{

      searchChildren{
        parent:t="suspension_01_01"
        find:t="bone_wheel_"
        occurence:i=1
        limitMin:p3=-1.0, -1.0, 0.0
        limitMax:p3=1.0, 1.0, 0.0
      }
    }

    wheel_02{

      searchChildren{
        parent:t="suspension_01_01"
        find:t="bone_wheel_"
        occurence:i=2
        limitMin:p3=-1.0, -1.0, 0.0
        limitMax:p3=1.0, 1.0, 0.0
      }
    }

    wheel_03{

      searchChildren{
        parent:t="suspension_01_02"
        find:t="bone_wheel_"
        occurence:i=1
        limitMin:p3=-1.0, -1.0, 0.0
        limitMax:p3=1.0, 1.0, 0.0
      }
    }

    wheel_04{

      searchChildren{
        parent:t="suspension_01_02"
        find:t="bone_wheel_"
        occurence:i=2
        limitMin:p3=-1.0, -1.0, 0.0
        limitMax:p3=1.0, 1.0, 0.0
      }
    }

    wheel_06{

      searchChildren{
        occurence:i=2
        limitMin:p3=0.0, 0.0, 0.0
        limitMax:p3=0.0, 0.0, 0.0
        parent:t="lever"
        find:t="bone_wheel_"
      }
    }

    wheel_drive{

      searchChildren{
        occurence:i=1
        limitMin:p3=0.0, 0.0, 0.0
        limitMax:p3=0.0, 0.0, 0.0
        parent:t="lever"
        find:t="bone_wheel_"
      }
    }
  }

  constraints{

    direction{
      point:t="suspension_01_01"
      from:t="wheel_01"
      to:t="wheel_02"
    }

    direction{
      point:t="suspension_01_02"
      from:t="wheel_03"
      to:t="wheel_04"
    }

    direction{
      moveNodes:b=yes
      rotateSecond:b=no
      moveSecond:b=yes
      moveOnly:b=no
      from:t="wheel_06"
      to:t="wheel_drive"
      point:t="lever"
    }

    projection{
      point:t="suspension_01_01"
      from:t="wheel_01"
      to:t="wheel_02"
      moveEdge:b=yes
    }

    projection{
      point:t="suspension_01_02"
      from:t="wheel_03"
      to:t="wheel_04"
      moveEdge:b=yes
    }

    projection{
      point:t="lever"
      from:t="suspension_01_01"
      to:t="suspension_01_02"
      moveEdge:b=yes
    }

    projection{
      rotateSecond:b=no
      moveSecond:b=yes
      moveOnly:b=no
      moveEdge:b=yes
      from:t="wheel_06"
      to:t="wheel_drive"
      point:t="lever"
    }
  }
}