Weapon{
  trigger:t="gunner0"
  blk:t="gameData/Weapons/groundModels_weapons/20mm_Type98_user_cannon.blk"
  emitter:t="bone_gun_barrel_02"
  flash:t="emtr_gun_flame_02"
  useEmitter:b=yes
  recoilOffset:r=0.045
  recoilMultiplier:r=0.7
  barrelDP:t="gun_barrel_02_dm"
  breechDP:t="cannon_breech_02_dm"
  recoilOffset:r=0.25
  recoilMultiplier:r=0.7
  defaultYaw:r=0.0
  defaultPitch:r=0.0
  speedYaw:r=40.0
  speedPitch:r=25.0
  bullets:i=260
  fireConeAngle:r=15.0
  ChainfireTime:r=3.0
  DelayAfterShoot:r=3.0
  accuracyAir:r=1.5
  accuracyGnd:r=1.0
  errMeasureVel:r=0.0
  errMeasureVelFast:r=0.0
  errMeasureVelFwdShift:r=0.0
  errMeasureVelDir:r=0.0
  errTargettingOn100kmph:r=0.0
  errTargetting:r=3.0
  errExplTime:r=0.0

  turret{
    head:t="bone_turret"
    gun:t="bone_gun"
    barrel:t="bone_gun_barrel_02"
    gunnerDm:t="gunner_dm"
    verDriveDm:t="drive_turret_v_dm"
    horDriveDm:t="drive_turret_h_dm"

    linkedAnimPart{
      verNode:t="bone_gun_roller_01"
      verLimits:p2=-10.0, 85.0
      verOriginLimits:p2=-10.0, 85.0
    }

    linkedAnimPart{
      verNode:t="bone_gun_roller_02"
      verLimits:p2=-10.0, 85.0
      verOriginLimits:p2=-10.0, 85.0
    }

    linkedAnimPart{
      verNode:t="bone_gun_roller_03"
      verLimits:p2=-10.0, 83.0
      verOriginLimits:p2=-10.0, 85.0
    }
  }

  limits{
    yaw:p2=-180.0, 180.0
    pitch:p2=-10.0, 85.0
  }

  limitsTable{
    lim1:p4=-180.0, -125.0, 8.0, 85.0
    lim2:p4=-125.0, 125.0, -10.0, 85.0
    lim3:p4=125.0, 180.0, 8.0, 85.0
  }
}

