cannon:b=no
weaponType:i=3
bUseHookAsRel:b=yes
emitColor:p3=1.0, 1.0, 0.0
emitI:r=10.0
emitR:r=3.0
emitTime:r=0.03
aimMinDist:r=0.1
aimMaxDist:r=1000.0
maxDeltaAngle:r=0.08
maxDeltaAngleVertical:r=0.08
shotFreq:r=12.0
traceFreq:i=3
bEnablePause:b=no
bullets:i=120
bulletsCluster:i=1
reloadTime:r=20.0
bulletsCartridge:i=200
fxType:t="small_gun_flame"
fxMultipleSpawn:b=yes
fxScale:r=0.2
shouldCollideWithRendinsts:b=yes
sound_path:t="tanks/weapon_tanks/cannons"
sound:t="20mm_tnsh"
sound_inside:t="20mm_tnsh_interior"
soundLoop:b=yes
sfxReloadBullet:t="grd_cannon_belt_reload"
isBulletBelt:b=yes

bullet{
  mass:r=0.096
  caliber:r=0.02
  speed:r=800.0
  maxDistance:r=2000.0
  bulletType:t="apcr"
  hitPowerMult:r=20.0
  relativeVelHitShift:p2=300.0, 1000.0
  armorFragmentsDamage:b=yes
  armorFragmentsDamageRadius:r=1.0
  armorFragmentsDamageAngle:r=15.0
  armorFragmentsAmount:i=15
  armorFragmentsArmorPower:r=2.0
  armorFragmentsHit:r=8.0
  relativeVelArmorShift:p2=200.0, 1000.0
  groundCollisionEffect:t="ground_splash"
  groundFxScale:p2=0.6, 0.7
  ricochetEffect:t="cannon_ricochet"
  waterCollisionEffect:t="water_splash"

  hitpower{
    HitPower0m:p2=0.85, 500.0
    HitPower10000m:p2=0.8, 10000.0
  }

  armorpower{
    ArmorPower0m:p2=35.0, 10.0
    ArmorPower100m:p2=34.0, 100.0
    ArmorPower500m:p2=27.0, 500.0
    ArmorPower1000m:p2=20.0, 1000.0
    ArmorPower1500m:p2=10.0, 1500.0
    ArmorPower2000m:p2=8.0, 2000.0
    ArmorPower2500m:p2=3.0, 2500.0
  }

  visual{
    range_percent_of_ammo:p2=100.0, 0.0
    traceFreq:i=1
    tracer:t="noColor"
  }

  collisions{

    steel{
      fx:t="cannon_hit_light"
    }

    armor{
      fx:t="cannon_hit_armor"
    }
  }
}

