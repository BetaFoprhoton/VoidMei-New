cannon:b=yes
weaponType:i=3
bUseHookAsRel:b=yes
emitColor:p3=1.0, 1.0, 0.0
emitI:r=10.0
emitR:r=3.0
emitTime:r=0.03
aimMinDist:r=0.1
aimMaxDist:r=3000.0
maxDeltaAngle:r=0.3
maxDeltaAngleVertical:r=0.6
shotFreq:r=5.0
shotFreqRndK:r=0.2
traceFreq:i=3
bEnablePause:b=no
bullets:i=250
bulletsCluster:i=1
reloadTime:r=15.0
bulletsCartridge:i=65
fxType:t="small_gun_flame"
fxMultipleSpawn:b=yes
fxScale:r=0.2
shouldCollideWithRendinsts:b=yes
sfxReloadBullet:t="grd_cannon_belt_reload"
notUseDefaultBulletInGui:b=yes

bullet{
  mass:r=0.28
  caliber:r=0.025
  speed:r=900.0
  maxDistance:r=5000.0
  bulletType:t="ap_t"
  bulletName:t="25mm_br_132"
  hitPowerMult:r=35.0
  relativeVelHitShift:p2=300.0, 1000.0
  relativeVelArmorShift:p2=200.0, 1000.0
  groundCollisionEffect:t="ground_splash"
  groundFxScale:p2=0.95, 1.05
  waterFxScale:p2=1.0, 1.2
  ricochetEffect:t="cannon_ricochet"
  waterCollisionEffect:t="20_ap_water_splash"

  hitpower{
    HitPower0m:p2=0.85, 500.0
    HitPower10000m:p2=0.8, 10000.0
  }

  armorpower{
    ArmorPower0m:p2=46.0, 10.0
    ArmorPower100m:p2=43.0, 100.0
    ArmorPower500m:p2=33.0, 500.0
    ArmorPower1000m:p2=27.0, 1000.0
    ArmorPower1500m:p2=23.0, 1500.0
    ArmorPower2000m:p2=15.0, 2000.0
    ArmorPower2500m:p2=11.0, 2500.0
    ArmorPower5000m:p2=5.0, 5000.0
  }

  visual{
    range_percent_of_ammo:p2=100.0, 0.0
    traceFreq:i=1
    tracer:t="30mmRed"
    trail:t="black"
  }

  collisions{

    steel{
      fx:t="cannon_hit_light"
    }

    armor{
      fx:t="cannon_hit_armor"
    }

    wood{
      fx:t="old_ship_hit_ball"
    }
  }
}

